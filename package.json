{
  "name": "art",
  "version": "1.0.0",
  "description": "art企业中台组件库",
  "keywords": [
    "ant",
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component",
    "ui"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "directories": {
    "es": "es"
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "docs": "start-storybook -p 8080",
    "build:docs": "build-storybook --no-dll --quiet --docs",
    "build:types": "tsc -p tsconfig.build.json && cpr lib es",
    "build:umd": "webpack --config build/webpack.prod.js --progress",
    "build": "yarn run clean && yarn run build:types && yarn run gulp && yarn run build:umd",
    "test": "jest --config jest.config.js",
    "gulp": "cd build && gulp",
    "test:update": "jest --config jest.config.js --updateSnapshot",
    "gc": "git-cz",
    "clean": "rimraf dist lib es",
    "stylelint:fix": "stylelint './src/**/*.less' --syntax less --fix",
    "eslint:fix": "eslint  --color --fix ./src/components",
    "release": "ts-node ./scripts/release.ts"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "author": "vnues",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.2.5",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.4.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.2",
    "@types/semver": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "autoprefixer": "^9.8.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "conventional-changelog-cli": "^2.0.34",
    "cpr": "^3.0.1",
    "css-loader": "^3.6.0",
    "cz-customizable": "^6.2.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "express": "^4.17.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-filesize": "^0.0.6",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "less-loader": "^6.1.2",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-hot-loader": "^4.12.21",
    "rimraf": "^3.0.2",
    "semver": "^7.3.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^3.0.6",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-progress-bar": "^1.2.1"
  },
  "lint-staged": {
    "src/components/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "jest --config jest.config.js --passWithNoTests",
      "git add"
    ],
    "*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
